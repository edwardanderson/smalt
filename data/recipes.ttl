user:1.64 a schema:Recipe ;
    rdfs:label "Kobalt blaauw of smalt" ;
    schema:about <http://vocab.getty.edu/aat/300013293> ;
    schema:author [ a schema:Person ;
            rdfs:label "M.P.L. Bouvier" ] ;
    schema:dateCreated "1831"^^schema:Date ;
    schema:identifier "1.64" ;
    schema:inLanguage [ a schema:Language ;
            rdfs:label "Dutch" ] ;
    schema:isPartOf <https://id.rijksmuseum.nl/30161774> ;
    schema:subjectOf [ a schema:Comment ;
            schema:author <mailto:p.v.l@rijksmuseum.nl> ;
            schema:text "Writes about the difference in price with ultramarine; best smalt comes from Basel (not the Netherlands?); 7/8 francs per ounce; talks about drying properties; you can mix it with other paints to dry quickly as it doesnt really affect the colour that much; describes hemelsblauw or bleu d’azur as a poor grade of smalt; also describes blauwe asschen and Antwerpsch blaauw to be adulterated; explicitly mentions adding to brown paints to let them dry more quickly; further on also writes about different mullers and those made of glass, stating not to worry about any glass that enters the paint as it won’t affect the colour and only maybe affect the drying rate; states to not use cooked oil and smalt together! p591; buy your ultramarine in Rome, not in France or Germany where it is often adulterated with smalt; quantity of smalt as a drier: 1/20th in an ultramarine paint;" ] ;
    schema:title "Kobalt blaauw of smalt" ;
    schema:translationOfWork [ a schema:Recipe ;
            schema:dateCreated "1827"^^schema:Date ;
            schema:inLanguage [ a schema:Language ;
                    rdfs:label "French" ] ] ;
    schema:translator [ a schema:Person ;
            rdfs:label "J.C. Beijer" ] .

